%YAML 1.1
%TAG ! tag:oblong.com,2009:slaw/
--- !protein
descrips:
- sluice
- prot-spec v1.0
- request
- new-fluoro-template
ingests:
  name: Tweets
  icon: niteworks/images/twitter_icon_180.png
  type: network
  display-text: 'Twitter'
  attributes:
  - name: enable-menu
    selection-type: exclusive
    contents:
    - name: true
      display-text: Enable
      selected: true
    - name: false
      display-text: Disable
      selected: false
  - name: enabled-kinds
    display-text: 'Filter Options'
    contents:
    - name: tweet
      display-text: "Tweets"
      selected: true
      icon: tweet.png
    selection-type: inclusive
  default_shaders:
    path:
      fragment: ribbons.frag
      vertex: marching_ribbons.vert
    point:
      fragment: network.frag
      vertex: network.vert
  kinds:
  - tweet
  pigments:
  - name: tweet
    icon: niteworks/images/twitter.png
    size:
      type: SoftFloat
      value: 1.5
    type: point
  hover:
    max-found: 256
    min-zoom: 10
    glyph: "sluice/sharkmouth.png"
...
